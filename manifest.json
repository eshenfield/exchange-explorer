{
  "manifest_version": 2,

  "name": "Office 365 Explorer",
  "description": "This extension does its best to determine if the current domain uses Office 365",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
    "default_icon": "img/exchange-explorer-logo.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://dns-api.org/",
    "activeTab"
  ]
}
